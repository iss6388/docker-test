{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "api",
      "image": "093859584130.dkr.ecr.ap-northeast-2.amazonaws.com/docker",
      "hostname": "api",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 5000
        }
      ]
    },
    {
      "name": "api2",
      "image": "093859584130.dkr.ecr.ap-northeast-2.amazonaws.com/docker2",
      "hostname": "api2",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort": 3000,
          "containerPort": 3000
        }
      ]
    }
  ]
}
